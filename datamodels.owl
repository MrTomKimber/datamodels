#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#AbstractIsRepresentedBy
dm:AbstractIsRepresentedBy rdf:type owl:ObjectProperty ;
                           owl:inverseOf dm:ClassIsRepresentationOfAbstract ;
                           rdfs:domain dm:Abstract ;
                           rdfs:range [ owl:intersectionOf ( dm:Class
                                                             [ rdf:type owl:Class ;
                                                               owl:complementOf dm:Abstract
                                                             ]
                                                           ) ;
                                        rdf:type owl:Class
                                      ] .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#AttributeIsMemberOfClass
dm:AttributeIsMemberOfClass rdf:type owl:ObjectProperty ;
                            owl:inverseOf dm:ClassHasAttribute ;
                            rdfs:domain dm:Attribute ;
                            rdfs:range dm:Class .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#ClassHasAttribute
dm:ClassHasAttribute rdf:type owl:ObjectProperty ;
                     rdfs:domain dm:Class ;
                     rdfs:range dm:Attribute .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#ClassHasRelationshipWith
dm:ClassHasRelationshipWith rdf:type owl:ObjectProperty ;
                            rdfs:domain dm:Class ;
                            rdfs:range dm:Class ;
                            owl:propertyChainAxiom ( [ owl:inverseOf dm:RelationshipHasDomain
                                                     ]
                                                     dm:RelationshipHasRange
                                                   ) ;
                            <http://www.w3.org/2004/02/skos/core#definition> "This is an Inferred Relationship: ClassHasRelationshipWith provides a simplified direct pointer to classes linked via a Relationship." .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#ClassIsDomainOfRelationship
dm:ClassIsDomainOfRelationship rdf:type owl:ObjectProperty ;
                               owl:inverseOf dm:RelationshipHasDomain ;
                               rdfs:domain dm:Class ;
                               rdfs:range dm:Relationship .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#ClassIsMemberOfModel
dm:ClassIsMemberOfModel rdf:type owl:ObjectProperty ;
                        owl:inverseOf dm:ModelContainsClass ;
                        rdfs:domain dm:Class ;
                        rdfs:range dm:Model .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#ClassIsRangeOfRelationship
dm:ClassIsRangeOfRelationship rdf:type owl:ObjectProperty ;
                              owl:inverseOf dm:RelationshipHasRange ;
                              rdfs:domain dm:Class ;
                              rdfs:range dm:Relationship .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#ClassIsRepresentationOfAbstract
dm:ClassIsRepresentationOfAbstract rdf:type owl:ObjectProperty ;
                                   rdfs:domain [ owl:intersectionOf ( dm:Class
                                                                      [ rdf:type owl:Class ;
                                                                        owl:complementOf dm:Abstract
                                                                      ]
                                                                    ) ;
                                                 rdf:type owl:Class
                                               ] ;
                                   rdfs:range dm:Abstract .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#ModelContainsClass
dm:ModelContainsClass rdf:type owl:ObjectProperty ;
                      rdfs:domain dm:Model ;
                      rdfs:range dm:Class .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#ModelDomainContainsModel
dm:ModelDomainContainsModel rdf:type owl:ObjectProperty ;
                            owl:inverseOf dm:ModelIsMemberOfModelDomain ;
                            rdfs:domain dm:ModelDomain ;
                            rdfs:range dm:Model .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#ModelDomainContainsSubDomain
dm:ModelDomainContainsSubDomain rdf:type owl:ObjectProperty ;
                                owl:inverseOf dm:ModelDomainIsSubDomainOf ;
                                rdf:type owl:TransitiveProperty ;
                                rdfs:domain dm:ModelDomain ;
                                rdfs:range dm:ModelDomain .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#ModelDomainIsSubDomainOf
dm:ModelDomainIsSubDomainOf rdf:type owl:ObjectProperty ,
                                     owl:TransitiveProperty ;
                            rdfs:domain dm:ModelDomain ;
                            rdfs:range dm:ModelDomain .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#ModelIsMemberOfModelDomain
dm:ModelIsMemberOfModelDomain rdf:type owl:ObjectProperty ;
                              rdfs:domain dm:Model ;
                              rdfs:range dm:ModelDomain .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#RelationshipFromCardinality
dm:RelationshipFromCardinality rdf:type owl:ObjectProperty ;
                               rdfs:domain dm:Relationship ;
                               rdfs:range dm:Cardinality .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#RelationshipFromOptionality
dm:RelationshipFromOptionality rdf:type owl:ObjectProperty ;
                               rdfs:domain dm:Relationship ;
                               rdfs:range dm:Optionality .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#RelationshipHasDomain
dm:RelationshipHasDomain rdf:type owl:ObjectProperty ;
                         rdfs:domain dm:Relationship ;
                         rdfs:range dm:Class .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#RelationshipHasRange
dm:RelationshipHasRange rdf:type owl:ObjectProperty ;
                        rdfs:domain dm:Relationship ;
                        rdfs:range dm:Class .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#RelationshipKeyFrom
dm:RelationshipKeyFrom rdf:type owl:ObjectProperty ;
                       rdfs:domain dm:Relationship ;
                       rdfs:range dm:Attribute ;
                       <http://www.w3.org/2004/02/skos/core#definition> "The attribute that acts as a pointer (i.e. the foreign key) to the target entity for this relationship." .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#RelationshipKeyTo
dm:RelationshipKeyTo rdf:type owl:ObjectProperty ;
                     rdfs:domain dm:Relationship ;
                     rdfs:range dm:Attribute ;
                     rdfs:comment "The attribute that acts as the primary key for the entity being referenced in the relationship." .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#RelationshipToCardinality
dm:RelationshipToCardinality rdf:type owl:ObjectProperty ;
                             rdfs:domain dm:Relationship ;
                             rdfs:range dm:Cardinality .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#RelationshipToOptionality
dm:RelationshipToOptionality rdf:type owl:ObjectProperty ;
                             rdfs:domain dm:Relationship ;
                             rdfs:range dm:Optionality .


