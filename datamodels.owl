###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#Mandatory
dm:Mandatory rdf:type owl:NamedIndividual ,
                      dm:Optionality .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#Many
dm:Many rdf:type owl:NamedIndividual ,
                 dm:Cardinality .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#One
dm:One rdf:type owl:NamedIndividual ,
                dm:Cardinality .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#Optional
dm:Optional rdf:type owl:NamedIndividual ,
                     dm:Optionality .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#Person
dm:Person rdf:type owl:NamedIndividual ,
                   dm:Concept .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#PersonID
dm:PersonID rdf:type owl:NamedIndividual ,
                     dm:Attribute ;
            dm:AttributeIsMemberOfClass dm:Person .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#PersonName
dm:PersonName rdf:type owl:NamedIndividual ,
                       dm:Attribute ;
              dm:AttributeIsMemberOfClass dm:Person .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#Role
dm:Role rdf:type owl:NamedIndividual ,
                 dm:Concept .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#RoleID
dm:RoleID rdf:type owl:NamedIndividual ,
                   dm:Attribute ;
          dm:AttributeIsMemberOfClass dm:Role .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#RoleName
dm:RoleName rdf:type owl:NamedIndividual ,
                     dm:Attribute ;
            dm:AttributeIsMemberOfClass dm:Role .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#hasRole
dm:hasRole rdf:type owl:NamedIndividual ,
                    dm:Relationship ;
           dm:RelationshipHasDomain dm:Person ;
           dm:RelationshipHasRange dm:Role ;
           dm:RelationshipKeyFrom dm:PersonID ;
           dm:RelationshipKeyTo dm:RoleID .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#isOpenTo
dm:isOpenTo rdf:type owl:NamedIndividual ,
                     dm:Relationship ;
            dm:RelationshipHasDomain dm:Role ;
            dm:RelationshipHasRange dm:Person .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#isPerformedBy
dm:isPerformedBy rdf:type owl:NamedIndividual ,
                          dm:Relationship ;
                 dm:RelationshipHasDomain dm:Role ;
                 dm:RelationshipHasRange dm:Person .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( dm:Attribute
                dm:Class
                dm:Context
                dm:MetaReference
                dm:Model
                dm:ModelDomain
                dm:Relationship
              )
] .


