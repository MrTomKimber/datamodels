###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#Description
dm:Description rdf:type owl:DatatypeProperty ;
               rdfs:domain dm:Definition ;
               rdfs:range xsd:string .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#Name
dm:Name rdf:type owl:DatatypeProperty ;
        rdfs:domain dm:Definition ;
        rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#Abstract
dm:Abstract rdf:type owl:Class ;
            rdfs:subClassOf dm:Class ;
            <http://www.w3.org/2004/02/skos/core#definition> "An Abstract is a type of class used to represent a single abstracted version of classes that might be present across multiple models."^^xsd:string .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#Attribute
dm:Attribute rdf:type owl:Class ;
             rdfs:subClassOf dm:Definition ;
             rdfs:label "Attribute"^^xsd:string ;
             <http://www.w3.org/2004/02/skos/core#definition> "An attribute represents a particular data point associated with an entity. It will often have a value that conforms to a primitive data type - but for enumerations, might be associated with some other class or controlled list. For modelling purposes, use the attribute and express it as its primitive form, and consider annotating the associated Relationship relation between entities to reference the attributes used to perform the link."^^xsd:string .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#Cardinality
dm:Cardinality rdf:type owl:Class ;
               rdfs:subClassOf dm:MetaReference ;
               rdfs:label "Cardinality"^^xsd:string .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#Class
dm:Class rdf:type owl:Class ;
         rdfs:subClassOf dm:Definition ;
         rdfs:label "Class"^^xsd:string ;
         <http://www.w3.org/2004/02/skos/core#definition> "A class is a set of things having some property or attribute in common."^^xsd:string .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#Concept
dm:Concept rdf:type owl:Class ;
           rdfs:subClassOf dm:Class ;
           <http://www.w3.org/2004/02/skos/core#definition> "A concept in this context is a high level class describing some concept, idea or thing that is of interest to the business. Concepts form the nodes of a conceptual data model."^^xsd:string .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#ConceptualDataModel
dm:ConceptualDataModel rdf:type owl:Class ;
                       rdfs:subClassOf dm:Model .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#Context
dm:Context rdf:type owl:Class ;
           rdfs:subClassOf dm:Definition ;
           rdfs:label "Context"^^xsd:string ;
           <http://www.w3.org/2004/02/skos/core#definition> "A context is some grouping of entities, schema or concepts. In physical data models, this might describe an IBM DB2 or Oracle schema (i.e. a collection of tables, views and other objects into an administrative package), an application or a collection of datasets. For logical or conceptual models, it would be used to form an informal grouping within a larger model. For more formal groupings, expect to use Domain, or Model."^^xsd:string .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#DataType
dm:DataType rdf:type owl:Class ;
            rdfs:subClassOf dm:MetaReference ;
            rdfs:label "DataType"^^xsd:string .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#DataTypeSchema
dm:DataTypeSchema rdf:type owl:Class ;
                  rdfs:subClassOf dm:MetaReference ;
                  rdfs:label "DataTypeSchema"^^xsd:string .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#Definition
dm:Definition rdf:type owl:Class ;
              rdfs:label "Definition"^^xsd:string ;
              <http://www.w3.org/2004/02/skos/core#definition> "A definition is the description of some kind of representation."^^xsd:string .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#Entity
dm:Entity rdf:type owl:Class ;
          rdfs:subClassOf dm:Class ;
          <http://www.w3.org/2004/02/skos/core#definition> "An entity describes some business concept, defining a series of attributes commonly associated with it. Entities form the nodes of logical and physical data models."^^xsd:string .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#LogicalDataModel
dm:LogicalDataModel rdf:type owl:Class ;
                    rdfs:subClassOf dm:Model .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#MetaReference
dm:MetaReference rdf:type owl:Class .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#Model
dm:Model rdf:type owl:Class ;
         rdfs:subClassOf dm:Definition ;
         rdfs:label "Model"^^xsd:string .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#ModelDomain
dm:ModelDomain rdf:type owl:Class ;
               rdfs:subClassOf dm:Definition ;
               rdfs:label "ModelDomain"^^xsd:string .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#Optionality
dm:Optionality rdf:type owl:Class ;
               rdfs:subClassOf dm:MetaReference ;
               rdfs:label "Optionality"^^xsd:string .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#PhysicalDataModel
dm:PhysicalDataModel rdf:type owl:Class ;
                     rdfs:subClassOf dm:Model .


###  http://www.semanticweb.org/tomk/ontologies/2022/9/datamodel#Relationship
dm:Relationship rdf:type owl:Class ;
                rdfs:subClassOf dm:Definition ;
                rdfs:label "Relationship"^^xsd:string ;
                <http://www.w3.org/2004/02/skos/core#definition> """A relationship class describes a possible interaction, association or property shared between a pair of class definitions. 

Each *instance* of relationship class will have a Range and Domain associated with it that constrain the anticipated class types that are acceptable for the relationship."""^^xsd:string .


