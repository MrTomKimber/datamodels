    <!-- http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Asserts -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Asserts">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Posits"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Declaration"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Posit"/>
        <owl:propertyDisjointWith rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Refutes"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Atom -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Atom">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Posit"/>
        <rdfs:comment>An Atom is a single constituatn element of a triple.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#DiscourseContains -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#DiscourseContains">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Discourse"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Declaration"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Discourse"/>
        <rdfs:comment>A Discourse is a hierarchical container, whose members can be other sub-discourses, or root-level Declarations.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Object -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Object">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Atom"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Posit"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Posits -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Posits">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Declaration"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Posit"/>
        <rdfs:comment>A Declaration Posits some Posit. This relationship links the act of stating some posit, and the Posit itself. The same Posit can be Posited by multiple Declarations.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Predicate -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Predicate">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Atom"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Posit"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Proposes -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Proposes">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#isProposedBy"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Declaration"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Discourse"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Refutes -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Refutes">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Posits"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#IrreflexiveProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Declaration"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Posit"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Subject -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Subject">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Atom"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Posit"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#isProposedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#isProposedBy">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Declaration"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Discourse"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Digest -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Digest">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#Posit"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>The Digest is a longform string representation of a triple, where each Atom is expressed in n3 notation, delimited by 3 tildes &apos;~~~&apos;, and is normally assigned to a Posit.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#GeneratedOn -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/tomk/ontologies/2022/11/discourse#GeneratedOn">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
        <rdfs:comment>A DateTime value describing the datetime when the associated information was generated within the ontology.</rdfs:comment>
    </owl:DatatypeProperty>
    


